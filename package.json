{
  "name": "boilerplace-vite-react",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
    "preview": "vite preview",
    "format": "pnpm lint --fix && prettier '**/*.{js,ts,jsx,tsx}' --write --ignore-path .gitignore",
    "check-types": "tsc --noEmit --pretty",
    "commit": "cz",
    "prepare": "husky"
  },
  "dependencies": {
    "@mantine/carousel": "^7.15.1",
    "@mantine/core": "^7.15.1",
    "@mantine/dates": "^7.15.1",
    "@mantine/dropzone": "^7.15.1",
    "@mantine/form": "^7.15.1",
    "@mantine/hooks": "^7.15.1",
    "@mantine/modals": "^7.15.1",
    "@mantine/notifications": "^7.15.1",
    "@phosphor-icons/react": "^2.1.7",
    "axios": "^1.7.2",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "dayjs": "^1.11.13",
    "embla-carousel-react": "^8.5.1",
    "query-string": "^9.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.0.2",
    "tailwind-merge": "^2.3.0",
    "zod": "^3.23.8",
    "zustand": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@commitlint/types": "^19.0.3",
    "@types/node": "^20",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.2",
    "@typescript-eslint/eslint-plugin": "^7.16.1",
    "@typescript-eslint/parser": "^7.15.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "commitlint-config-gitmoji": "^2.3.1",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.4",
    "eslint": "^8.57.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jsx-a11y": "^6.9.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.3",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-tailwindcss": "^3.17.4",
    "eslint-plugin-unused-imports": "^4.0.0",
    "husky": "^9.0.11",
    "inquirer": "9.0.0",
    "lightningcss": "^1.26.0",
    "lint-staged": "^15.2.7",
    "postcss": "^8.4.39",
    "postcss-preset-mantine": "^1.15.0",
    "postcss-simple-vars": "^7.0.1",
    "prettier": "^3.3.2",
    "react-number-format": "^5.4.2",
    "rimraf": "^5.0.8",
    "sass": "^1.78.0",
    "tailwindcss": "^3.4.6",
    "typescript": "^5.2.2",
    "vc-cz-emoji": "^0.0.2",
    "vite": "^5.3.4",
    "vite-node": "^2.0.5",
    "vite-plugin-checker": "^0.7.2",
    "vite-plugin-svgr": "^4.3.0",
    "vite-tsconfig-paths": "^4.3.2"
  },
  "config": {
    "commitizen": {
      "path": "vc-cz-emoji"
    },
    "cz-emoji": {
      "types": [
        {
          "emoji": "🐛",
          "code": ":bug:",
          "description": "Fixing a bug.",
          "name": "fix"
        },
        {
          "emoji": "✨",
          "code": ":sparkles:",
          "description": "Introducing new features.",
          "name": "feat"
        },
        {
          "emoji": "⚡️",
          "code": ":zap:",
          "description": "Improving performance.",
          "name": "perf"
        },
        {
          "emoji": "🔥",
          "code": ":fire:",
          "description": "Removing code or files.",
          "name": "chore"
        },
        {
          "emoji": "📝",
          "code": ":memo:",
          "description": "Writing docs.",
          "name": "docs"
        },
        {
          "emoji": "🚀",
          "code": ":rocket:",
          "description": "Deploying stuff.",
          "name": "build"
        },
        {
          "emoji": "✅",
          "code": ":white_check_mark:",
          "description": "Adding tests.",
          "name": "test"
        },
        {
          "emoji": "🚧",
          "code": ":construction:",
          "description": "Work in progress.",
          "name": "wip"
        },
        {
          "emoji": "♻️",
          "code": ":recycle:",
          "description": "Refactoring code.",
          "name": "refactor"
        },
        {
          "emoji": "⏪",
          "code": ":rewind:",
          "description": "Reverting changes.",
          "name": "revert"
        },
        {
          "emoji": "🔀",
          "code": ":twisted_rightwards_arrows:",
          "description": "Merging branches.",
          "name": "merge"
        }
      ]
    }
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
